[dlabrepo-main]
name=DLab Repository
baseurl=REPOSITORY_AMAZON_MAIN/$basearch/
enabled=1
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-7
priority=1
sslverify=0

[dlabrepo-updates]
name=DLab Repository
baseurl=REPOSITORY_AMAZON_UPDATES/$basearch/
enabled=1
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-7
priority=1
sslverify=0